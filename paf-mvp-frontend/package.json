  },
  "homepage": "https://github.com/criteo/paf-mvp-demo/paf-mvp-frontend#readme",
  "dependencies": {
    "jest-environment-jsdom": "^28.1.3",
    "preact": "^10.5.15",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "tinyduration": "^3.2.2",
