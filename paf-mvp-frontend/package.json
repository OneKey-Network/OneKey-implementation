    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/preact": "^2.0.1",
    "@types/cypress": "^1.1.3",
    "@types/jest": "^28.1.3",
    "@types/react": "^17.0.35",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "cypress": "^9.5.3",
    "eslint": "^8.11.0",
