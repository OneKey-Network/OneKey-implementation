{
  "name": "@onekey/frontend",
  "version": "0.1.5",
  "description": "OneKey frontend library and widget",
  "main": "src/main.tsx",
  "scripts": {
    "start": "rollup -c -w",
    "build": "rollup -c",
    "test": "jest",
    "test:cypress": "wait-on http://localhost:3000 && cypress run",
    "cypress:wait-and-run": "run-p -r start test:cypress",
    "lint": "npx eslint ./src --c src/.eslintrc --no-eslintrc",
    "lint:fix": "npx eslint ./src --fix --c src/.eslintrc --no-eslintrc",
    "lint:prettier": "prettier --list-different \"./src/**/*.{tsx,ts}\"",
    "prettier": "prettier --write \"./src/**/*.{tsx,ts}\" --loglevel silent",
    "prepare": "npm run build"
  },
  "repository": {
